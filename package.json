{
    "name": "jsmusicdb-next",
    "version": "1.6.2",
    "scripts": {
        "start": "concurrent \"npm run watch\" \"npm run lite\" ",
        "clean": "rm -rf dist && mkdir dist",
        "build:sass": "node-sass -o dist/ app/",
        "tsc": "tsc",
        "lite": "lite-server",
        "typings": "typings",
        "postinstall": "typings install",
        "watch": "npm run clean && concurrent \"npm run watch:ts\" \"npm run watch:sass\"",
        "watch:ts": "tsc --watch",
        "watch:sass": "npm run build:sass && node-sass -o dist/ -w app/",
        "gulp": "gulp",
        "copy-global-dev": "gulp copy-global-dev",
        "rimraf": "rimraf",
        "deploy": "node-sass -o dist/ app/ && gulp build"
    },
    "license": "MIT",
    "dependencies": {
        "@angular/common": "^4.3.5",
        "@angular/compiler": "^4.3.5",
        "@angular/core": "^4.3.5",
        "@angular/forms": "^4.3.5",
        "@angular/http": "^4.3.5",
        "@angular/platform-browser": "^4.3.5",
        "@angular/platform-browser-dynamic": "^4.3.5",
        "@angular/router": "^4.3.5",
        "@angular/upgrade": "^4.3.5",
        "angular2-jwt": "^0.2.3",
        "bootstrap": "^3.3.7",
        "core-js": "^2.5.0",
        "hammerjs": "^2.0.8",
        "jsrsasign": "^8.0.3",
        "moment": "^2.18.1",
        "ng2-bootstrap": "^1.6.3",
        "ng2-sticky-kit": "^4.1.0",
        "pouchdb": "^6.3.4",
        "reflect-metadata": "^0.1.10",
        "rxjs": "^5.4.3",
        "systemjs": "^0.20.18",
        "winstrap": "^0.5.11",
        "zone.js": "^0.8.16"
    },
    "devDependencies": {
        "concurrently": "^3.5.0",
        "del": "^3.0.0",
        "gulp": "^3.9.1",
        "gulp-autoprefixer": "^4.0.0",
        "gulp-clean-css": "^3.7.0",
        "gulp-concat": "^2.6.1",
        "gulp-csso": "^3.0.0",
        "gulp-inline-ng2-template": "^4.0.0",
        "gulp-plumber": "^1.1.0",
        "gulp-rename": "^1.2.2",
        "gulp-replace": "^0.6.1",
        "gulp-rev": "^8.0.0",
        "gulp-rev-replace": "^0.4.3",
        "gulp-typescript": "^3.2.1",
        "lite-server": "^2.3.0",
        "ng2-youtube-player": "^0.0.3",
        "node-sass": "^4.5.3",
        "penthouse": "^0.11.12",
        "rimraf": "^2.6.1",
        "run-sequence": "^2.1.0",
        "systemjs-builder": "^0.16.9",
        "tslint": "^5.6.0",
        "typescript": "^2.4.2",
        "typings": "^2.1.1"
    },
    "author": "Lucien Immink",
    "description": "Web version of musicdb next",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/lucienimmink/JSMusicDBNext"
    }
}
