{
  "name": "jsmusicdb-next",
  "version": "2.12.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "npm run --silent sass:dark && ng serve",
    "sass": "node-sass",
    "sass:dark": "node-sass --output-style compressed --omit-source-map-url ./src/sass/dark.scss > ./src/global/css/dark.css",
    "build": "npm run --silent sass:dark && ng build --prod --i18n-locale=\"en-GB\"",
    "lint": "tslint --fix --force -p .",
    "postversion": "npm run --silent build && npm publish && git push && git push --tags"
  },
  "files": [
    "dist"
  ],
  "author": "Lucien Immink",
  "description": "Web version of musicdb next",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lucienimmink/JSMusicDBNext"
  },
  "dependencies": {
    "@angular/animations": "^7.2.12",
    "@angular/common": "^7.2.12",
    "@angular/compiler": "^7.2.12",
    "@angular/core": "^7.2.12",
    "@angular/forms": "^7.2.12",
    "@angular/http": "^7.2.12",
    "@angular/platform-browser": "^7.2.12",
    "@angular/platform-browser-dynamic": "^7.2.12",
    "@angular/router": "^7.2.12",
    "@thebespokepixel/es-tinycolor": "^1.0.4",
    "bootstrap": "^4.3.1",
    "core-js": "^2.6.5",
    "fast-average-color": "^4.3.0",
    "hammerjs": "^2.0.8",
    "idb-keyval": "^3.2.0",
    "jsrsasign": "^8.0.12",
    "ngx-bootstrap": "^4.0.1",
    "node-sass": "^4.11.0",
    "rxjs": "^6.4.0",
    "winstrap": "^0.5.12",
    "zone.js": "^0.9.0"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^7.4.3",
    "@angular-devkit/build-angular": "^0.13.8",
    "@angular/cli": "^7.3.8",
    "@angular/compiler-cli": "^7.2.12",
    "@angular/language-service": "^7.2.12",
    "@isaac.frontend/stylelint-config-scss": "^1.1.1",
    "@types/node": "^11.13.4",
    "copy-webpack-plugin": "^5.0.2",
    "glob-all": "^3.1.0",
    "prettier-stylelint": "^0.4.2",
    "purgecss-webpack-plugin": "^1.4.0",
    "stylelint": "^9.10.1",
    "ts-node": "^8.0.3",
    "tslint": "^5.15.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "~3.2.4"
  }
}
