{
  "name": "jsmusicdb-next",
  "version": "1.0.2",
  "scripts": {
    "start": "concurrent \"npm run watch\" \"npm run lite\" ",
    "clean": "rm -rf dist && mkdir dist",
    "build:sass": "node-sass -o dist/ app/",
    "tsc": "tsc",
    "lite": "lite-server",
    "typings": "typings",
    "postinstall": "typings install",
    "watch": "npm run clean && concurrent \"npm run watch:ts\" \"npm run watch:sass\"",
    "watch:ts": "tsc --watch",
    "watch:sass": "npm run build:sass && node-sass -o dist/ -w app/",
    "gulp": "gulp",
    "copy-global-dev": "gulp copy-global-dev",
    "rimraf": "rimraf",
    "deploy": "node-sass -o dist/ app/ && gulp build"
  },
  "license": "MIT",
  "dependencies": {
    "@angular/animations": "^4.0.1",
    "@angular/common": "^4.0.1",
    "@angular/compiler": "^4.0.1",
    "@angular/compiler-cli": "^4.0.1",
    "@angular/core": "^4.0.1",
    "@angular/forms": "^4.0.1",
    "@angular/http": "^4.0.1",
    "@angular/platform-browser": "^4.0.1",
    "@angular/platform-browser-dynamic": "^4.0.1",
    "@angular/platform-server": "^4.0.1",
    "@angular/router": "^4.0.1",
    "@angular/upgrade": "^4.0.1",
    "angular2-jwt": "^0.2.0",
    "bootstrap": "^3.3.7",
    "core-js": "^2.4.1",
    "hammerjs": "^2.0.8",
    "jsrsasign": "^7.1.2",
    "moment": "^2.18.1",
    "ng2-bootstrap": "1.6.3",
    "pouchdb": "^6.1.2",
    "reflect-metadata": "^0.1.10",
    "rxjs": "^5.3.0",
    "systemjs": "^0.20.12",
    "winstrap": "^0.5.11",
    "zone.js": "^0.8.5"
  },
  "devDependencies": {
    "concurrently": "^3.4.0",
    "del": "^2.2.2",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^3.1.1",
    "gulp-concat": "^2.6.1",
    "gulp-inline-ng2-template": "^4.0.0",
    "gulp-plumber": "^1.1.0",
    "gulp-rename": "^1.2.2",
    "gulp-replace": "^0.5.4",
    "gulp-rev": "^7.1.2",
    "gulp-rev-replace": "^0.4.3",
    "gulp-typescript": "^3.1.6",
    "lite-server": "^2.3.0",
    "node-sass": "^4.5.2",
    "rimraf": "^2.6.1",
    "rollup": "^0.41.6",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-uglify": "^1.0.1",
    "run-sequence": "^1.2.2",
    "systemjs-builder": "^0.16.4",
    "tslint": "^5.0.0",
    "typescript": "^2.2.2",
    "typings": "^2.1.0"
  },
  "author": "Lucien Immink",
  "description": "Web version of musicdb next",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lucienimmink/JSMusicDBNext"
  }
}
