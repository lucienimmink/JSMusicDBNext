{
  "name": "jsmusicdb-next",
  "version": "2.8.1",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "npm run --silent sass:dark && ng serve",
    "sass": "node-sass",
    "sass:dark": "node-sass --output-style compressed --omit-source-map-url ./src/sass/dark.scss > ./src/global/css/dark.css",
    "build": "npm run --silent sass:dark && ng build --prod --i18n-locale=\"en-GB\" --subresource-integrity"
  },
  "author": "Lucien Immink",
  "description": "Web version of musicdb next",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lucienimmink/JSMusicDBNext"
  },
  "dependencies": {
    "@angular/animations": "6.1.9",
    "@angular/common": "6.1.9",
    "@angular/compiler": "6.1.9",
    "@angular/core": "6.1.9",
    "@angular/forms": "6.1.9",
    "@angular/http": "6.1.9",
    "@angular/platform-browser": "6.1.9",
    "@angular/platform-browser-dynamic": "6.1.9",
    "@angular/router": "6.1.9",
    "@angular/service-worker": "6.1.9",
    "@types/youtube": "^0.0.34",
    "bootstrap": "^4.1.3",
    "core-js": "^2.5.7",
    "hammerjs": "^2.0.8",
    "idb-keyval": "^3.1.0",
    "jsrsasign": "^8.0.12",
    "ngx-bootstrap": "^3.0.1",
    "ngx-youtube-player": "^6.0.0",
    "node-sass": "^4.9.3",
