{
  "name": "jsmusicdb-next",
  "version": "2.11.3",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "npm run --silent sass:dark && ng serve",
    "sass": "node-sass",
    "sass:dark": "node-sass --output-style compressed --omit-source-map-url ./src/sass/dark.scss > ./src/global/css/dark.css",
    "build": "npm run --silent sass:dark && ng build --prod --i18n-locale=\"en-GB\"",
    "postversion": "npm run --silent build && npm publish"
  },
  "files": [
    "dist"
  ],
  "author": "Lucien Immink",
  "description": "Web version of musicdb next",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lucienimmink/JSMusicDBNext"
  },
  "dependencies": {
    "@angular/animations": "^7.2.8",
    "@angular/common": "^7.2.8",
    "@angular/compiler": "^7.2.8",
    "@angular/core": "^7.2.8",
    "@angular/forms": "^7.2.8",
    "@angular/http": "^7.2.8",
    "@angular/platform-browser": "^7.2.8",
    "@angular/platform-browser-dynamic": "^7.2.8",
    "@angular/router": "^7.2.8",
    "@thebespokepixel/es-tinycolor": "^1.0.4",
    "@types/youtube": "^0.0.34",
    "bootstrap": "^4.3.1",
    "core-js": "^2.6.5",
    "fast-average-color": "^4.3.0",
    "hammerjs": "^2.0.8",
    "idb-keyval": "^3.1.0",
    "jsrsasign": "^8.0.12",
    "ngx-bootstrap": "^3.2.0",
    "ngx-youtube-player": "^6.0.0",
    "node-sass": "^4.11.0",
    "rxjs": "^6.4.0",
    "winstrap": "^0.5.12",
    "zone.js": "^0.8.29"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^7.4.2",
    "@angular-devkit/build-angular": "^0.13.5",
    "@angular/cli": "^7.3.5",
    "@angular/compiler-cli": "^7.2.8",
    "@angular/language-service": "^7.2.8",
    "@isaac.frontend/stylelint-config-scss": "^1.1.1",
    "@types/node": "^11.11.1",
    "copy-webpack-plugin": "^4.6.0",
    "glob-all": "^3.1.0",
    "prettier-stylelint": "^0.4.2",
