{
  "name": "jsmusicdb-next",
  "version": "2.5.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "npm run --silent sass:dark && ng serve",
    "sass": "node-sass",
    "sass:dark": "node-sass --output-style nested ./src/sass/dark.scss > ./src/global/css/dark.css",
    "sass:dark:minimal": "node-sass --output-style compressed ./src/sass/dark.scss > ./src/global/css/dark.css",
    "prod": "npm run --silent sass:dark:minimal && ng build -prod --locale=\"en-GB\""
  },
  "author": "Lucien Immink",
  "description": "Web version of musicdb next",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lucienimmink/JSMusicDBNext"
  },
  "dependencies": {
    "@angular/animations": "5.2.10",
    "@angular/common": "5.2.10",
    "@angular/compiler": "5.2.10",
    "@angular/core": "5.2.10",
    "@angular/forms": "5.2.10",
    "@angular/http": "5.2.10",
    "@angular/platform-browser": "5.2.10",
    "@angular/platform-browser-dynamic": "5.2.10",
    "@angular/router": "5.2.10",
    "@angular/service-worker": "5.2.10",
    "@types/youtube": "^0.0.32",
    "bootstrap-sass": "^3.3.7",
    "core-js": "^2.5.5",
    "hammerjs": "^2.0.8",
    "idb-keyval": "^3.0.3",
    "jsrsasign": "^8.0.12",
    "ng2-sticky-kit": "^5.3.0",
    "ngx-bootstrap": "^2.0.4",
    "ngx-youtube-player": "^0.1.0",
    "node-sass": "^4.8.3",
    "rxjs": "^5.5.10",
    "winstrap": "^0.5.11",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular/cli": "1.7.4",
    "@angular/compiler-cli": "5.2.10",
    "@angular/language-service": "5.2.10",
    "@types/node": "^9.6.6",
    "ts-node": "^5.0.1",
    "tslint": "^5.9.1",
    "typescript": "2.6.2"
  }
}
