{
  "name": "jsmusicdb-next",
  "version": "0.1.0",
  "scripts": {
    "start": "concurrent \"npm run watch\" \"npm run lite\" ",
    "clean": "rm -rf dist && mkdir dist",
    "build:sass": "node-sass -o dist/ app/",
    "tsc": "tsc",
    "lite": "lite-server",
    "typings": "typings",
    "postinstall": "typings install",
    "watch": "npm run clean && concurrent \"npm run watch:ts\" \"npm run watch:sass\"",
    "watch:ts": "tsc --watch",
    "watch:sass": "npm run build:sass && node-sass -o dist/ -w app/",
    "gulp": "gulp",
    "rimraf": "rimraf",
    "deploy": "node-sass -o dist/ app/ && gulp build"
  },
  "license": "MIT",
  "dependencies": {
    "@angular/common": "^2.4.1",
    "@angular/compiler": "^2.4.1",
    "@angular/core": "^2.4.1",
    "@angular/forms": "^2.4.1",
    "@angular/http": "^2.4.1",
    "@angular/platform-browser": "^2.4.1",
    "@angular/platform-browser-dynamic": "^2.4.1",
    "@angular/router": "^3.4.1",
    "@angular/upgrade": "^2.4.1",
    "angular2-jwt": "git+https://github.com/alexduros/angular2-jwt.git",
    "bootstrap": "^3.3.7",
    "core-js": "^2.4.1",
    "hammerjs": "^2.0.8",
    "jsrsasign": "^7.0.0",
    "lodash": "^4.17.3",
    "moment": "^2.17.1",
    "ng2-bootstrap": "^1.1.16-10",
    "pouchdb": "^6.1.0",
    "reflect-metadata": "^0.1.9",
    "rxjs": "^5.0.2",
    "systemjs": "^0.19.41",
    "winstrap": "^0.5.11",
    "zone.js": "^0.7.4"
  },
  "devDependencies": {
    "concurrently": "^3.1.0",
    "del": "^2.2.2",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^3.1.1",
    "gulp-concat": "^2.6.1",
    "gulp-inline-ng2-template": "^4.0.0",
    "gulp-plumber": "^1.1.0",
    "gulp-rename": "^1.2.2",
    "gulp-replace": "^0.5.4",
    "gulp-rev": "^7.1.2",
    "gulp-rev-replace": "^0.4.3",
    "gulp-typescript": "^3.1.3",
    "lite-server": "^2.2.2",
    "node-sass": "^4.1.1",
    "rimraf": "^2.5.4",
    "run-sequence": "^1.2.2",
    "systemjs-builder": "^0.15.34",
    "tslint": "^4.2.0",
    "typescript": "^2.1.4",
    "typings": "^2.1.0"
  },
  "author": "Lucien Immink",
  "description": "Web version of musicdb next",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lucienimmink/JSMusicDBNext"
  }
}
