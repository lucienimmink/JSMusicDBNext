    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.8.4",
    "@angular/cli": "^6.2.4",
    "@angular/compiler-cli": "6.1.9",
    "@angular/language-service": "6.1.9",
    "@isaac.frontend/stylelint-config-scss": "^1.1.1",
    "@types/node": "^10.11.6",
    "prettier-stylelint": "^0.4.2",
    "stylelint": "^9.6.0",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "typescript": "^3.1.1"
  }
