{
    "name": "jsmusicdb-next",
    "version": "0.2.0",
    "scripts": {
        "start": "concurrent \"npm run watch\" \"npm run lite\" ",
        "clean": "rm -rf dist && mkdir dist",
        "build:sass": "node-sass -o dist/ app/",
        "tsc": "tsc",
        "lite": "lite-server",
        "typings": "typings",
        "postinstall": "typings install",
        "watch": "npm run clean && concurrent \"npm run watch:ts\" \"npm run watch:sass\"",
        "watch:ts": "tsc --watch",
        "watch:sass": "npm run build:sass && node-sass -o dist/ -w app/",
        "gulp": "gulp",
        "copy-global-dev": "gulp copy-global-dev",
        "rimraf": "rimraf",
        "deploy": "node-sass -o dist/ app/ && gulp build"
    },
    "license": "MIT",
    "dependencies": {
        "@angular/common": "^4.0.0-rc.2",
        "@angular/compiler": "^4.0.0-rc.2",
        "@angular/core": "^4.0.0-rc.2",
        "@angular/forms": "^4.0.0-rc.2",
        "@angular/http": "^4.0.0-rc.2",
        "@angular/platform-browser": "^4.0.0-rc.2",
        "@angular/platform-browser-dynamic": "^4.0.0-rc.2",
        "@angular/router": "^4.0.0-rc.2",
        "@angular/upgrade": "^4.0.0-rc.2",
        "angular2-jwt": "^0.1.28",
        "bootstrap": "^3.3.7",
        "core-js": "^2.4.1",
        "hammerjs": "^2.0.8",
        "jsrsasign": "^7.1.0",
        "lodash": "^4.17.4",
        "moment": "^2.17.1",
        "ng2-bootstrap": "^1.3.3",
        "pouchdb": "^6.1.2",
        "reflect-metadata": "^0.1.9",
        "rxjs": "^5.1.1",
        "systemjs": "^0.20.9",
        "winstrap": "^0.5.11",
        "zone.js": "^0.7.7"
    },
    "devDependencies": {
        "concurrently": "^3.1.0",
        "del": "^2.2.2",
        "gulp": "^3.9.1",
        "gulp-autoprefixer": "^3.1.1",
        "gulp-concat": "^2.6.1",
        "gulp-inline-ng2-template": "^4.0.0",
        "gulp-plumber": "^1.1.0",
        "gulp-rename": "^1.2.2",
        "gulp-replace": "^0.5.4",
        "gulp-rev": "^7.1.2",
        "gulp-rev-replace": "^0.4.3",
        "gulp-typescript": "^3.1.5",
        "lite-server": "^2.2.2",
        "node-sass": "^4.5.0",
        "rimraf": "^2.5.4",
        "run-sequence": "^1.2.2",
        "systemjs-builder": "^0.16.3",
        "tslint": "^4.4.2",
        "typescript": "^2.1.6",
        "typings": "^2.1.0"
    },
    "author": "Lucien Immink",
    "description": "Web version of musicdb next",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/lucienimmink/JSMusicDBNext"
    }
}