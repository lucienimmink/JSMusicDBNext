{
  "name": "jsmusicdb-next",
  "version": "0.0.1",
  "scripts": {
    "start": "concurrent \"npm run watch\" \"npm run lite\" ",
    "clean": "rm -rf dist && mkdir dist",
    "build:sass": "node-sass -o dist/ app/",
    "tsc": "tsc",
    "lite": "lite-server",
    "typings": "typings",
    "postinstall": "typings install",
    "watch": "npm run clean && concurrent \"npm run watch:ts\" \"npm run watch:sass\"",
    "watch:ts": "tsc --watch",
    "watch:sass": "npm run build:sass && node-sass -o dist/ -w app/"
  },
  "license": "MIT",
  "dependencies": {
    "@angular/common": "2.0.0-rc.3",
    "@angular/compiler": "2.0.0-rc.3",
    "@angular/core": "2.0.0-rc.3",
    "@angular/forms": "^0.1.1",
    "@angular/http": "2.0.0-rc.3",
    "@angular/platform-browser": "2.0.0-rc.3",
    "@angular/platform-browser-dynamic": "2.0.0-rc.3",
    "@angular/router-deprecated": "2.0.0-rc.2",
    "@angular/upgrade": "2.0.0-rc.3",
    "angular2-jwt": "^0.1.16",
    "bootstrap": "^3.3.6",
    "core-js": "^2.4.0",
    "lodash": "^4.13.1",
    "moment": "^2.13.0",
    "musicdbcore": "git+https://git@github.com/lucienimmink/MusicDBCore.git",
    "ng2-bootstrap": "^1.0.17",
    "ng2-vs-for": "^1.1.1",
    "node-sass": "^3.8.0",
    "pouchdb": "^5.4.5",
    "reflect-metadata": "^0.1.3",
    "rxjs": "5.0.0-beta.6",
    "systemjs": "0.19.27",
    "tslint": "^3.13.0",
    "winstrap": "^0.5.10",
    "zone.js": "^0.6.12"
  },
  "devDependencies": {
    "concurrently": "^2.1.0",
    "lite-server": "^2.2.0",
    "typescript": "^1.8.10",
    "typings": "^1.1.0"
  },
  "author": "Lucien Immink",
  "description": "Web version of musicdb next",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lucienimmink/JSMusicDBNext"
  }
}
