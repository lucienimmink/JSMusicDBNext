{
  "name": "jsmusicdb-next",
  "version": "2.10.1",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "npm run --silent sass:dark && ng serve",
    "sass": "node-sass",
    "sass:dark": "node-sass --output-style compressed --omit-source-map-url ./src/sass/dark.scss > ./src/global/css/dark.css",
    "build": "npm run --silent sass:dark && ng build --prod --i18n-locale=\"en-GB\" --subresource-integrity",
    "postversion": "npm run --silent build && npm publish"
  },
  "author": "Lucien Immink",
  "description": "Web version of musicdb next",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lucienimmink/JSMusicDBNext"
  },
  "dependencies": {
    "@angular/animations": "7.2.1",
    "@angular/common": "7.2.1",
    "@angular/compiler": "7.2.1",
    "@angular/core": "7.2.1",
    "@angular/forms": "7.2.1",
    "@angular/http": "7.2.1",
    "@angular/platform-browser": "7.2.1",
    "@angular/platform-browser-dynamic": "7.2.1",
    "@angular/router": "7.2.1",
    "@thebespokepixel/es-tinycolor": "^0.3.1",
    "@types/youtube": "^0.0.34",
    "bootstrap": "^4.2.1",
    "core-js": "^2.6.2",
    "fast-average-color": "^4.3.0",
    "hammerjs": "^2.0.8",
    "idb-keyval": "^3.1.0",
    "jsrsasign": "^8.0.12",
    "ngx-bootstrap": "^3.1.4",
    "ngx-youtube-player": "^6.0.0",
    "node-sass": "^4.11.0",
    "rxjs": "^6.3.3",
    "winstrap": "^0.5.12",
    "zone.js": "^0.8.28"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^7.2.0",
    "@angular-devkit/build-angular": "^0.12.2",
    "@angular/cli": "^7.2.2",
    "@angular/compiler-cli": "7.2.1",
    "@angular/language-service": "7.2.1",
    "@isaac.frontend/stylelint-config-scss": "^1.1.1",
    "@types/node": "^10.12.18",
    "copy-webpack-plugin": "^4.6.0",
    "glob-all": "^3.1.0",
    "prettier-stylelint": "^0.4.2",
    "purify-css": "^1.2.5",
