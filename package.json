{
  "name": "jsmusicdb-next",
  "version": "2.6.1",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "npm run --silent sass:dark && ng serve",
    "sass": "node-sass",
    "sass:dark": "node-sass --output-style compressed --omit-source-map-url ./src/sass/dark.scss > ./src/global/css/dark.css",
    "sass:dark:minimal": "node-sass --output-style compressed --omit-source-map-url ./src/sass/dark.scss > ./src/global/css/dark.css",
    "prod": "npm run --silent sass:dark:minimal && ng build --prod --i18n-locale=\"en-GB\""
  },
  "author": "Lucien Immink",
  "description": "Web version of musicdb next",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lucienimmink/JSMusicDBNext"
  },
  "dependencies": {
    "@angular/animations": "6.0.0",
    "@angular/common": "6.0.0",
    "@angular/compiler": "6.0.0",
    "@angular/core": "6.0.0",
    "@angular/forms": "6.0.0",
    "@angular/http": "6.0.0",
    "@angular/platform-browser": "6.0.0",
    "@angular/platform-browser-dynamic": "6.0.0",
    "@angular/router": "6.0.0",
    "@angular/service-worker": "6.0.0",
    "@types/youtube": "^0.0.32",
    "bootstrap-sass": "^3.3.7",
    "core-js": "^2.5.5",
    "hammerjs": "^2.0.8",
    "idb-keyval": "^3.0.3",
    "jsrsasign": "^8.0.12",
    "ng2-sticky-kit": "^5.3.0",
    "ngx-bootstrap": "^2.0.4",
    "ngx-youtube-player": "^0.1.0",
    "node-sass": "^4.8.3",
    "rxjs": "^6.1.0",
    "rxjs-compat": "^6.1.0",
    "winstrap": "^0.5.11",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.6.0",
    "@angular/cli": "6.0.0",
    "@angular/compiler-cli": "6.0.0",
    "@angular/language-service": "6.0.0",
    "@types/node": "^9.6.6",
    "ts-node": "^5.0.1",
    "tslint": "^5.9.1",
    "typescript": "^2.7.2"
  }
}
